{
  "name": "Vuetron",
  "version": "1.0.0",
  "description": "Vue/Vuex debugging app built on Electron",
  "main": "electron.js",
  "scripts": {
    "test": "tape ./test/specs/*.spec.js -r ./test/helpers/setup.js | tap-spec",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --quiet --port 3000",
    "dev": "cross-env NODE_ENV=development electron ./",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "package": "npm run build; npm run package-osx; npm run package-win; npm run package-linux;",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/vuetron-icon.png.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"vuetron\"",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "package-linux": "electron-packager .  Vuetron --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/256x256.png --prune=true --out=release-builds",
    "create-installer-debian": "electron-installer-debian --src release-builds/Vuetron-linux-x64/ --arch amd64 --config debian.json",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/vuetron-icon.png.icns --prune=true --out=release-builds",
    "create-installer-osx": "electron-installer-dmg ./release-builds/Vuetron-darwin-x64/Vuetron.app vuetron"
  },
  "author": "Samantha Salley",
  "license": "ISC",
  "dependencies": {
    "electron": "^15.5.5",
    "electron-devtools-installer": "^2.2.1",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vued3tree": "^3.4.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-uglify": "^1.0.2",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^1.0.2",
    "browser-env": "^3.2.4",
    "bufferutil": "^3.0.3",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "deep-diff": "^0.3.8",
    "electron-installer-debian": "^0.7.1",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^9.1.0",
    "electron-reload": "^1.2.2",
    "file-loader": "^1.1.5",
    "path": "^0.12.7",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.1",
    "socket.io": "^2.0.4",
    "style-loader": "^0.19.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "url-loader": "^0.6.2",
    "utf-8-validate": "^3.0.4",
    "vue-awesome": "^2.3.4",
    "vue-devtools": "^3.1.9",
    "vue-html": "^0.3.0",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "vue-test-utils": "^1.0.0-beta.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-node-externals": "^1.6.0"
  }
}
